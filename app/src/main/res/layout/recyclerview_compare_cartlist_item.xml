<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="150dp"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="130dp"
        android:layout_height="130dp"
        app:srcCompat="@drawable/exampleofmart" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/compare_cartlist_martname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="훼미리 마트"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp" />

        <TextView
            android:id="@+id/compare_cartlist_itempresence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="모든 장바구니 재고 있음"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="10dp"/>

        <TextView
            android:id="@+id/compare_cartlist_totalprice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="23,000원"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"/>

    </LinearLayout>



</LinearLayout>